{
  "name": "@openfn/language-mysql",
  "version": "1.2.0",
  "description": "A MySQL Language Pack for OpenFn",
  "homepage": "https://docs.openfn.org",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenFn/language-mysql.git"
  },
  "scripts": {
    "build": "node_modules/.bin/babel src -d lib && npm run ast",
    "test": "mocha --require @babel/register",
    "test:watch": "mocha -w --require @babel/register",
    "ast": "simple-ast --adaptor ./src/Adaptor.js --output ast.json",
    "postversion": "git push && git push --tags",
    "version": "npm run build && git add -A lib ast.json"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "lib/",
    "ast.json"
  ],
  "dependencies": {
    "@openfn/language-common": "1.4.1",
    "json-sql": "^0.3.10",
    "mysql": "^2.13.0",
    "squel": "^5.8.0",
    "string-escape": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/register": "^7.9.0",
    "@openfn/simple-ast": "^0.3.2",
    "assertion-error": "^1.0.1",
    "chai": "^3.4.0",
    "deep-eql": "^0.1.3",
    "jsdoc": "^3.6.6",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "sinon": "^1.17.2"
  },
  "directories": {
    "lib": "./lib"
  },
  "bundledDependencies": [
    "@openfn/language-common",
    "json-sql",
    "mysql",
    "squel",
    "string-escape"
  ]
}
